<div id="clients_collection">
  <%= render RubyUI::Table.new(id: :organization_clients) do %>
    <%= render RubyUI::TableHeader.new do %>
      <%= render RubyUI::TableRow.new(class: "text-gray-500" ) do %>
        <%= render RubyUI::TableHead.new(class: "w-1/3") { t("common.name") } %>
        <%= render RubyUI::TableHead.new(class: "w-1/3") { t("common.rate") } %>
        <%= render RubyUI::TableHead.new(class: "w-1/3 text-end") { t("common.actions") } %>
      <% end %>
    <% end %>
      <%= render partial: "workspace/projects/client", collection: clients %>
  <% end %>
</div>