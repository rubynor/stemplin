<!-- Open Graph data -->
<meta property="og:title" content="<%= t 'open_graph.title' %>" />
<meta property="og:type" content="website" />
<meta property="og:url" content="<%= t 'open_graph.url' %>" />
<meta property="og:image" content="<%= image_path "open_graph_logo.png" %>" />
<meta property="og:image:type" content="image/png" />
<meta property="og:image:alt" content="<%= t 'open_graph.image_alt' %>" />
<meta property="og:description" content="<%= t 'open_graph.description' %>" name="description" />
<meta property="og:site_name" content="<%= t 'open_graph.site_name' %>" />
<meta property="og:locale" content="en_US" />
